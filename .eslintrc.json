{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double", {"avoidEscape": true}],
        "no-extra-semi": "off",
        "semi":"off"
    }
}
